Config { font = "xft:Monaco:size=7"
       , bgColor  = "#0a0c0f"
       , fgColor  = "white"
       , position = Top
       , lowerOnStart = True
       , commands = [ Run StdinReader
		    , Run CpuFreq ["-t","[<cpu0>|<cpu1>]","-L","0","-H","2","-l","lightblue","-n","white","-h","red"] 50
                    , Run Date "%a, %b %d | %H:%M" "date" 10 ]
       , sepChar  = "%"
       , alignSep = "}{"
       , template = "%StdinReader% }{ %cpufreq% %date%"
       }
